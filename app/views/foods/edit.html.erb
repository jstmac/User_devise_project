<h1>Edit Food</h1>

<%= form_with(model: @food, local: true) do |f| %>
  <div class="field">
    <%= f.label :item %>
    <%= f.text_field :item, required: true, placeholder: "Item" %>
  </div>
  <div class="field">
    <%= f.label :group%>
    <%= f.text_field :group, required: true, placeholder:"Group"%>
  </div>
  <%= f.submit %>
<% end %>